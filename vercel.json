{
  "version": 2,
  "functions": {
    "src/main.ts": {
      "runtime": "@vercel/node",
      "includeFiles": "public/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/api",
      "dest": "src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ]
}